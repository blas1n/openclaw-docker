{
  "gateway": {
    "mode": "local",
    "bind": "tailnet",
    "port": 18789,
    "trustedProxies": ["YOUR_TAILSCALE_IP"],
    "auth": {
      "mode": "token",
      "token": "YOUR_AUTH_TOKEN_HEX64",
      "allowTailscale": true
    }
  },
  "discovery": {
    "mdns": { "mode": "minimal" }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "botToken": "YOUR_TELEGRAM_BOT_TOKEN",
      "streamMode": "partial",
      "dmPolicy": "allowlist",
      "allowFrom": [0],
      "groupPolicy": "allowlist"
    }
  },
  "models": {
    "providers": {
      "ollama": {
        "baseUrl": "http://host.docker.internal:11434/v1",
        "apiKey": "ollama-local",
        "api": "openai-completions",
        "models": [
          {
            "id": "YOUR_MODEL:TAG",
            "name": "YOUR_MODEL:TAG"
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "workspace": "/home/node/.openclaw/workspace",
      "model": {
        "primary": "ollama/YOUR_MODEL:TAG"
      },
      "sandbox": {
        "mode": "all",
        "scope": "session",
        "workspaceAccess": "rw",
        "docker": {
          "image": "node:20-bookworm-slim",
          "readOnlyRoot": true,
          "network": "bridge",
          "binds": [
            "/home/node/.openclaw/agents/main/sessions:/home/node/.openclaw/agents/main/sessions:ro",
            "/home/node/.openclaw/workspace:/home/node/.openclaw/workspace:rw"
          ],
          "env": {}
        }
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    }
  },
  "session": {
    "dmScope": "per-channel-peer",
    "reset": {
      "idleMinutes": 60
    }
  },
  "tools": {
    "deny": ["process"],
    "elevated": {
      "enabled": true
    }
  },
  "browser": {
    "enabled": true,
    "defaultProfile": "openclaw",
    "evaluateEnabled": false,
    "headless": true,
    "noSandbox": false
  },
  "skills": {
    "entries": {
      "tavily": {
        "enabled": true
      }
    }
  },
  "plugins": {
    "entries": {
      "telegram": {
        "enabled": true
      }
    }
  },
  "logging": {
    "level": "info",
    "redactSensitive": "tools",
    "redactPatterns": [
      "sk-[a-zA-Z0-9]+",
      "sk-ant-[a-zA-Z0-9-]+",
      "[0-9]{10}:[A-Za-z0-9_-]+"
    ]
  }
}
